<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>myredbox</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<description></description>
  	<display-name>ListMoviesServlet</display-name>
  	<servlet-name>ListMoviesServlet</servlet-name>
  	<servlet-class>com.training.java.redbox.ListMoviesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListMoviesServlet</servlet-name>
  	<url-pattern>/listMovies</url-pattern>
  </servlet-mapping>
</web-app>